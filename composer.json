{
    "name": "satupersen/http",
    "description": "The Illuminate Http package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "satupersen/session": "^1.0",
        "satupersen/support": "^1.0",
        "symfony/http-foundation": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/mime": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Satupersen\\Http\\": ""
        }
    },
    "suggest": {
        "ext-gd": "Required to use Satupersen\\Http\\Testing\\FileFactory::image()."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
